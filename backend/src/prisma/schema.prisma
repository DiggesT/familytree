generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  nick      String   @unique
  password  String
  createdAt DateTime @default(now())
}

model Member {
  id         String   @id @default(uuid())
  lastName   String
  firstName  String
  middleName String
  text       String
  mother     String
  father     String
  createdAt  DateTime @default(now())
}
