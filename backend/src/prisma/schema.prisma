generator client {
  provider = "prisma-client-js"
  previewFeatures = ["fullTextSearchPostgres"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  nick      String   @unique
  name      String   @default("")
  password  String
  createdAt DateTime @default(now())
  members   Member[]
}

model Member {
  id           String   @id @default(uuid())
  serialNumber Int      @unique @default(autoincrement())
  lastName     String
  firstName    String
  middleName   String
  text         String
  mother       String
  father       String
  createdAt    DateTime @default(now())
  createdBy    String
  creator      User     @relation(fields: [createdBy], references: [id])
}
